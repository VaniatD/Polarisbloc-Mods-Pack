<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/TraderKindDef[defName = "Orbital_Polarisbloc"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationAdd">
      <xpath>/Defs/TraderKindDef[defName = "Orbital_Polarisbloc"]/stockGenerators</xpath>
      <value>
        <li Class="StockGenerator_SingleDef">
          <thingDef>Vanya_ConsciousnessCube</thingDef>
          <price>Exorbitant</price>
          <countRange>
            <min>-3</min>
            <max>1</max>
          </countRange>
        </li>
        <li Class="StockGenerator_SingleDef">
          <thingDef>Vanya_StabPowerCore</thingDef>
          <price>Exorbitant</price>
          <countRange>
            <min>-3</min>
            <max>1</max>
          </countRange>
        </li>
        <li Class="StockGenerator_SingleDef">
          <thingDef>Vanya_CombatRations</thingDef>
          <countRange>
            <min>10</min>
            <max>50</max>
          </countRange>
        </li>
      </value>
    </match>
  </Operation>
</Patch>